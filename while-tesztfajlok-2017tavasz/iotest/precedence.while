program plap
begin
	write(1000);
	write(2*3+6);
	write(6+2*3);
	write(2*3-6);
	write(6-2*3);
	write(1100);
	write(3 div 2+1);
	write(1+3 div 2);
	write(3 div 2-1);
	write(1-3 div 2);
	write(1200);
	write(3 mod 2+1);
	write(1+3 mod 2);
	write(3 mod 2-1);
	write(1-3 mod 2);
	write(1300);
	write(false and false or true);
	write(true or false and false);
end
